{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "App": {
    "Self": {
      "JwtAuthority": "http://localhost:5004",
      "DeleteTokenLifeTimeInDays": 90, //When a user request deletion of his account, this is the max days before the request is invalid; Default: 90
      "IdentityApplications": [ //These settings only apply the first time the API runs (these values don't update the db)
        {
          "Key": "dex-api",
          //TODO: This should be set via an environment variable
          "Value": "secret"
        },
        {
          "Key": "dex-frontend",
          "Value": "secret"
        }
      ]

    },
    "Api": {
      "DeXApiUrl": "http://localhost:5000"
    },
    "Fhict": {
      "FhictIdentityUrl": "https://identity.fhict.nl",
      "FhictClientId": "",
      "FhictClientSecret": "",
      "FhictRedirectUri": "",
      "FhictScopes": ""
    }
    },
  "AllowedHosts": "*"
}
